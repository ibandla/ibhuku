<div class="container">
	<h6 class="dress"><a href="index.html">Home</a> <i> </i> Register </h6>
</div>
<div class="back">
	<h2>REGISTER</h2>
</div>
		<!---->
		<div class="container">
		<div class="register">
		<h3>INFORMATION</h3>
		  	  <form ng-submit="submitRegistration(registrationForm)" role="form" ng-init="registrationForm = {}">
				{{itemArray}}
					
					<div class="mation">
					
						
					
					

					<div class="form-group">
						 <label>Title</label>
						 <!--<input type="text">-->
						 <ui-select ng-model="registrationForm.title_id" required="required" class="form-control">
    <ui-select-match>
        <span ng-bind="$select.selected.name"></span>
    </ui-select-match>
    <ui-select-choices repeat="title.id as title in (titles | filter: $select.search) track by title.id">
        <span ng-bind="title.name"></span>
    </ui-select-choices>
</ui-select>
</div>
					<div class="form-group">
						 <label>Name</label>
						 <!--<input type="text">-->
						 <input  name="name" ng-model="registrationForm.name" required="required" class="form-control"/>
						 </div>

					 <div class="form-group">
						 <label>Email Address</label>
						 <!--<input type="text">-->
						 <input type="email" name="email" ng-model="registrationForm.email" required="required" class="form-control"/>
						 </div>
						 <div class="form-group">
							<label>Password</label>
							<input type="password" name="password" ng-model="registrationForm.password" required="required" class="form-control"/>
							<!--<input type="text">-->					 
					</div>
					  <div class="form-group">
					    <label>Password Confirmation</label>
					    <input type="password" name="password_confirmation" ng-model="registrationForm.password_confirmation" required="required" class="form-control"/>
					  </div>
										 </div>
 <div ng-hide="showsubmit" vc-recaptcha ng-model="registrationForm.grecaptcharesponse" on-success="setResponse(response)" on-expire="cbExpiration()"></div>
			<input ng-show="showsubmit" type="submit"  ng-model="handleRegBtnClick" value="submit">


 
				
				</form>
				
		   </div>
			</div>
			
