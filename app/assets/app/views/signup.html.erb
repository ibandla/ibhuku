<div class="container">
	<div class="account_grid">
		<div class=" login-right">
			<h3>INFORMATION</h3>

			<form ng-submit="submitRegistration(registrationForm)">
				<formly-form model="registrationForm" fields="registrationForm.fields">
				<p>NOTE: Password must be at least 8 characters, contain big, small letters and digits</p>

					<div ng-hide="showsubmit" vc-recaptcha ng-model="registrationForm.grecaptcharesponse" on-success="setResponse(response)"
					    on-expire="cbExpiration()"></div>
						<%# ng-show="showsubmit"  %>
					<a class="forgot" ui-sref="reset">Forgot Your Password?&nbsp;</a><input  type="submit"
					    ng-model="handleRegBtnClick" value="Submit">
				</formly-form>
			</form>
		</div>
		<div class="padd">


		</div>


	</div>
</div>