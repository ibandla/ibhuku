



<body class="active_admin logged_out new">
<div id="wrapper">

  <div id="content_wrapper">
    <div id="active_admin_content">
      <div id="login">

<h2><%= I18n.t('submit_token_title', {:scope => 'devise'}) %></h2>



<%= form_for(resource, :as => resource_name, :url => [resource_name, :checkga], :html => { :method => :put }) do |f| %>
	<%= f.hidden_field :tmpid, {:value => @tmpid} %>

	<li style="list-style-type: none;" class="password input required stringish" id="admin_new_password_input">

  <p><%= f.label :gauth_token, I18n.t('enter_token', {:scope => 'devise.registration'}) %><abbr title="required">*</abbr><br />
  <%= f.text_field :gauth_token, :autocomplete => :off%>
  </li>



<fieldset class="actions">
  <li style="list-style-type: none;" class="action input_action " id="admin_password_submit_action">
  <%= f.submit I18n.t('submit_token', {:scope => 'devise'}) %>
  </li>
  </fieldset>
	

	
<% end %>






</div>

    </div>
  </div>
  <div id="footer">

  </div>
</div>


</body>
